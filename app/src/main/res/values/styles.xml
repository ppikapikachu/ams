<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">

    <style name="full_screen_theme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="hLinetwo">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#FFFFFF</item>
    </style>
    <style name="et">
        <item name="android:layout_width">120px</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">3dp</item>
<!--        <item name="android:background">@null</item>-->
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="tv">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18dp</item>
    </style>
</resources>